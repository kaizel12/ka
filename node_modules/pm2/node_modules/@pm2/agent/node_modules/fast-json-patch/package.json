  "main": "index.js",
  "module": "index.mjs",
  "typings": "index.d.ts",
  "devDependencies": {
    "benchmark": "^2.1.4",
    "bluebird": "^3.5.5",
    "bluebird-retry": "^0.11.0",
    "chalk": "^2.4.2",
